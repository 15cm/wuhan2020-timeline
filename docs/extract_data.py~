


file_input=open('index.md','r')
file_output=open('_data/news.yml','w')

file_input.readline()
file_input.readline()
file_input.readline()

for line in file_input:
    if line[0:2] == "##":
        print('date: '+line)
    elif line[0:1]= "【":
        print('group '+ line)


